<script type="text/javascript">
    
    this.enableExtensition = function( telephoneId , extId )
    {
        var attrVal =  $("input[id*='"+telephoneId+"']")[0].value;
        var extVal	=	$("input[id*='"+extId+"']")[0].value;
         console.log('=====>>>>111'+extVal);
          console.log('=====>>>>'+attrVal);
        if(attrVal.length>0)
        {
             console.log('=====>>>>'+attrVal.length);
            $("input[id*='"+extId+"']")[0].readOnly = false;
        }else{
            $("input[id*='"+extId+"']")[0].readOnly = true;
            $("input[id*='"+extId+"']")[0].value	= '';
        }
       
    }
    
</script>
        

            <script>
                jQuery.noConflict();
            function toggleContactRow(rowId) {
                try {
                    var HeadTable = jQuery(document.getElementById(rowId + ":Img"));
                    var FirstTable = jQuery(document.getElementById(rowId + ":First"));
                    var SecondTable = jQuery(document.getElementById(rowId + "Second"));
                    var ThirdTable = jQuery(document.getElementById(rowId + ":Third"));
                    var FourthTable = jQuery(document.getElementById(rowId + ":Fourth"));
                    var FifthTable = jQuery(document.getElementById(rowId + ":Fifth"));
                    
                    if (HeadTable.hasClass("hideContacts")) {
                        HeadTable.attr("class", "showContacts");
                        FirstTable.hide();
                        SecondTable.hide();
                        ThirdTable.hide();
                        FourthTable.hide();
                        FifthTable.hide();
                    }
                    else {
                        HeadTable.attr("class", "hideContacts");
                        FirstTable.show();
                        SecondTable.show();
                        ThirdTable.show();
                        FourthTable.show();
                        FifthTable.show();
                    }
                }
                catch (e) { alert(e); }
            }              
            function toggleSelectAll(elCheckbox, rowId) {
                try {
                    var SelectAll = jQuery(elCheckbox);
                    var ContactsRow = jQuery(document.getElementById(rowId + ":Contact"));
                    ContactsRow.find(".ContactCheckbox").each(function() {
                        jQuery(this).prop("checked", SelectAll.is(":checked"));
                    });
                }
                catch (e) { alert(e); }
            }            
            </script>           

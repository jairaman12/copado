<apex:page controller="ProviderDetails_Extended" tabStyle="Account" sidebar="false" lightningStylesheets="true" title="Provider Details" >
    <apex:includeScript value="{!$Resource.jqueryMin}"/>
    <link class="user" href="https://cdn.datatables.net/1.10.15/css/jquery.dataTables.min.css" rel="stylesheet" type="text/css"/>   
    <script src="https://cdn.datatables.net/1.10.15/js/jquery.dataTables.min.js"></script>  
    <apex:includeScript value="{!$Resource.js4_paginationPage}"/>
        <apex:stylesheet value="{!$Resource.css4_paginationPage}"/> 
            <script>
            /* Initialize the number of records to show in pagination */
            var numberOfRecordsToShow = parseInt({!numberOfRecordsToShow});
    
    
    	$(document).ready(function(){
        
            /* Used to show/hide the Provider Tax Identification Section. Had to do this way because of rendering problem in Lightning */
            $("img[id*='TaxIdSection']").click(function() {
                $("[id*=TaxIdOutputPanel]").toggle();
            });
        
    	});
    </script>    
    <style>
        .lex_background {
            background-color: rgb(175, 197, 222);
            height: -webkit-fill-available;
            background-image: url(/_slds/images/themes/oneSalesforce/banner-brand-default.png?cache=210.2);
            background-repeat: repeat-x;
            background-position: top left;
        }
        body .pbBody table.list tr.headerRow td, body .pbBody table.list tr.headerRow th {
        	background-color: rgb(250, 250, 249);
        }
    </style>
    <apex:form styleClass="lex_background">
        <apex:outputPanel rendered="{!accountList[0].RecordType.name == 'Person Account' || accountList[0].RecordType.name == 'Physician' || accountList[0].RecordType.name == 'Rolodex'}">
            <style>
                .sfdcBody .accountMru .mruIcon, .sfdcBody .bRelatedList .accountBlock .relatedListIcon, .sfdcBody.accountTab .pageTitleIcon        
                {
                    background-position: initial;
                    background-image: url('/img/icon/t4v35/standard/person_account_60.png');
                    content: url('/img/icon/t4v35/standard/person_account_60.png');
                }
            </style>
        </apex:outputPanel>
        <apex:sectionHeader subtitle="{!accountList[0].name}" title="Provider details"/>
        <apex:pageBlock title="Provider Info">
            <style type="text/css">
                .showContacts {
                	background: transparent url('/img/icon/t4v35/standard/account_60.png') 0 0 no-repeat;
                }
                .hideContacts {
                	background: transparent url('/img/icon/t4v35/standard/account_60.png') 0 -11px no-repeat;
                }
            </style>
            <!--- Provider Information block -->
            <apex:pageBlockSection title="Provider Info" columns="2" rendered="{!IF(accountList.size ==0,false,true)}" collapsible="true" >
                <apex:outputField value="{!accountList[0].Name}" />
                <apex:outputField label="Parent Account" value="{!accountList[0].Parent.Name}" />
                <apex:outputField value="{!accountList[0].Class__c}" />
                <apex:outputField value="{!accountList[0].Organization_Type__c}" />
                <apex:outputField value="{!accountList[0].Website}" />
                <apex:outputField value="{!accountList[0].Designation__c}" />
                <apex:outputField value="{!accountList[0].Prov_ID__c}" />
                <apex:outputField value="{!accountList[0].CM_Corporate_Entity_Code__c}" />
                <apex:outputField value="{!accountList[0].Status__c}" />
            </apex:pageBlockSection>
            <!-- End of Provider Information block -->
            <!-- Provider Identification Info block -->
            <apex:pageblocksection title="Provider Identification Info" showheader="true" collapsible="true" columns="1" >
                <apex:pageBlockTable id="pii" value="{!provIdentificationList}" var="provIdentification" rendered="{!IF(provIdentificationList.size ==0,false,true)}">
                    <apex:column headerValue="PROVIDER ID INFO">
                        <apex:outputlink value="/{!provIdentification.Id}">
                            <apex:outputtext value="{!provIdentification.Name}"/>
                        </apex:outputlink>
                    </apex:column>
                    <apex:column value="{!provIdentification.CM_Prvdr_Iden_Typ_Cd__c}" headerValue="ID TYPE" />
                    <apex:column value="{!provIdentification.CM_Issu_Ident_Nmbr__c}" headerValue="ID NUM TYPE" />
                    <apex:column value="{!provIdentification.CM_PI_NPI_Type_CD__c}" />
                    <apex:column value="{!provIdentification.CM_PI_State__c}" />
                    <apex:column value="{!provIdentification.CM_PI_Board_Code__c}" />
                    <apex:column value="{!provIdentification.CM_PI_License_Status__c}" />
                    <apex:column value="{!provIdentification.CM_PI_Effective_Date__c}" />
                    <apex:column value="{!provIdentification.CM_PI_End_Date__c}" />
                    <apex:column value="{!provIdentification.CM_PI_Valid__c}" />
                </apex:pageBlockTable>
            </apex:pageblocksection>            
            <!-- End of Provider Identification Info block -->
            
            
            <!--Provider Tax Identification Info block with Service and Payee address location -->
            <apex:pageBlockSection id="TaxIdSection" title="Provider Tax Identification Info" columns="1" collapsible="true" />

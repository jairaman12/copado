                                        <apex:outputLink value="/{!pro.Id}">
                                            <apex:outputText value="{!pro.Name}"></apex:outputText>
                                        </apex:outputLink>
                                    </td>                                    
                                    <td class="dataCell"><apex:outputField value="{!pro.CM_PTI_Tax_Id__c}" /></td> 
                                    <td class="dataCell"><apex:outputField value="{!pro.CM_PTI_Tax_Shared_Ind__c}" /></td>
                                    <td class="dataCell"><apex:outputField value="{!pro.CM_PTI_Prov_Name_From__c}" /></td>
                                    <td class="dataCell"><apex:outputField value="{!pro.CM_PTI_Prov_Id_Start_Date__c}" /></td>
                                    <td class="dataCell"><apex:outputField value="{!pro.CM_PTI_Prov_Id_End_Date__c}" /></td>

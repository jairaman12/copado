                        <thead>
                            <tr>
                                <th>{!$objectType.CM_Provider_Tax_Id__c.Fields.Name.Label}</th>
                                <th>{!$objectType.CM_Provider_Tax_Id__c.Fields.CM_PTI_Tax_Id__c.Label}</th>
                                <th>{!$objectType.CM_Provider_Tax_Id__c.Fields.CM_PTI_Tax_Shared_Ind__c.Label}</th>
                                <th>{!$objectType.CM_Provider_Tax_Id__c.Fields.CM_PTI_Prov_Name_From__c.Label}</th>
                                <th>{!$objectType.CM_Provider_Tax_Id__c.Fields.CM_PTI_Prov_Id_Start_Date__c.Label}</th>
                                <th>{!$objectType.CM_Provider_Tax_Id__c.Fields.CM_PTI_Prov_Id_End_Date__c.Label}</th>
                                <th>{!$objectType.CM_Provider_Tax_Id__c.Fields.CM_PTI_Tax_Type__c.Label}</th>                  
                            </tr>  

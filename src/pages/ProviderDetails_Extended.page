                                                                                                    <apex:variable var="index" value="{!index + 1}" />
                                                                                                </tr> 
                                                                                            </apex:outputPanel>   
                                                                                            <tr> 
                                                                                                <td class="dataCell">
                                                                                                    <apex:outputLink value="/{!ntwrkspl.Id}">
                                                                                                        <apex:outputText value="{!ntwrkspl.Name}">                                                                                                    </apex:outputText>

                    <div id="s1" align='right'>      
                        <button onclick="vfo.validateAndSubmitAllForm();" class="sButton" type="button">Submit Form</button>                                           
                        <apex:commandLink value="Submit Form" action="{!saveProvInfo}" id="postCaptchaSubmit"/>    
                    </div>    
                </apex:outputText>                    
                
                <apex:outputText rendered="{!IF(useCaptcha == false,true,false)}">                      
                    <div id="s1" align='right'>      
                        <apex:commandLink value="Submit Form" action="{!saveProvInfo}" id="noCaptchaSubmit" styleClass="sButton" onclick="return vfo.validateAndSubmitAllNoCaptcha();"/>    
                    </div>    
                </apex:outputText>   
                
                <!-- don't render Captcha if automated testing -->   
                <c:pi_captchaComponent rendered="{!useCaptcha}" publicKey="{!captchaPublicKey}"/>    
                
            </div>   
        </apex:pageBlock>                                                            
    </apex:form>       
